    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "drizzle-orm": "^0.32.2",
    "framer-motion": "^11.3.24",
    "lucide-react": "^0.424.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "next-themes": "^0.3.0",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
